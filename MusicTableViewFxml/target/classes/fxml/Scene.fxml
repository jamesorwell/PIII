<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="ku.piii.musictableviewfxml.FXMLController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding> 
    <Label style="-fx-font: NORMAL 20 Tahoma;" text="Music Collection"                
        GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="addressBook">
    </Label>
    <TableView fx:id="tableView" GridPane.columnIndex="0" 
        GridPane.rowIndex="1">
    </TableView>
    <Button GridPane.rowIndex="2" text="Click Me!" onAction="#handleButtonAction" fx:id="button" />

</GridPane>